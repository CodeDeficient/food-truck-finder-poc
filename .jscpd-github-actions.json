{
  "reporters": ["consoleFull", "json"],
  "output": "jscpd-report/github-actions/",
  "minLines": 5,
  "minTokens": 50,
  "absolute": true,
  "blame": true,
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/*.md",
    "**/build/**",
    "**/docs/**",
    "**/.clinerules/**",
    "**/.husky/**",
    "**/.next/**",
    "**/CodeAnt/**",
    "**/errors/**",
    "**/playwright-report/**",
    "**/test-results/**",
    "**/tests/**",
    "**/supabase/**",
    "**/*.d.ts"
  ],
  "gitignore": true,
  "formats": ["javascript", "typescript"]
}

{
  "theme": "Default",
  "selectedAuthType": "oauth-personal",
  "fileFiltering": {
  "respectGitIgnore": false,
  "enableRecursiveFileSearch": true
},
  "mcpServers": {
    "context7-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "timeout": 30000,
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs",
        "tavily-search"
      ],
      "disabled": false
    },
    "taskmaster-ai": {
      "command": "npx",
      "args": [
        "-y",
        "task-master-ai"
      ],
      "timeout": 30000,
      "autoApprove": [
        "initialize_project",
        "models",
        "parse_prd",
        "get_tasks",
        "get_task",
        "next_task",
        "complexity_report",
        "set_task_status",
        "generate",
        "add_task",
        "add_subtask",
        "update",
        "update_task",
        "update_subtask",
        "remove_task",
        "remove_subtask",
        "clear_subtasks",
        "analyze_project_complexity",
        "expand_task",
        "expand_all",
        "add_dependency",
        "remove_dependency",
        "validate_dependencies",
        "fix_dependencies"
      ],
      "disabled": false
    },
    "browser-tools-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@agentdeskai/browser-tools-mcp@latest"
      ],
      "timeout": 30000,
      "host": "localhost",
      "port": 3025,
      "autoApprove": [
        "getConsoleLogs",
        "getConsoleErrors",
        "getNetworkErrors",
        "getNetworkLogs",
        "takeScreenshot",
        "getSelectedElement",
        "wipeLogs",
        "runAccessibilityAudit",
        "runPerformanceAudit",
        "runSEOAudit",
        "runNextJSAudit",
        "runDebuggerMode",
        "runAuditMode",
        "runBestPracticesAudit"
      ],
      "disabled": false
    },
    "tavily-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.0"
      ],
      "timeout": 30000,
      "env": {
        "TAVILY_API_KEY": ""
      },
      "autoApprove": [
        "search",
        "tavily-search",
        "tavily-deep-research"
      ],
      "disabled": false
    },

    "supabase-mcp-server": {
      "command": "C:/Users/zabri/.local/bin/supabase-mcp-server.exe",
      "args": [],
      "timeout": 30000,
      "env": {
        "QUERY_API_KEY": "",
        "SUPABASE_PROJECT_REF": "",
        "SUPABASE_DB_PASSWORD": "",
        "SUPABASE_REGION": "",
        "SUPABASE_ACCESS_TOKEN": "",
        "SUPABASE_SERVICE_ROLE_KEY": ""
      },
      "autoApprove": [
        "get_schemas",
        "get_tables",
        "get_table_schema",
        "retrieve_migrations",
        "retrieve_logs",
        "execute_postgresql",
        "send_management_api_request",
        "get_management_api_spec",
        "get_auth_admin_methods_spec",
        "call_auth_admin_method",
        "live_dangerously",
        "confirm_destructive_operation"
      ],
      "disabled": false
    }
  }
}
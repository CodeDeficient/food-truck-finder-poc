{
  "reporters": ["consoleFull", "json"],
  "output": "jscpd-report/",
  "minLines": 5,
  "minTokens": 50,
  "absolute": true,
  "blame": true,
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/docs/**",
    "**/.clinerules/**",
    "**/.claude-thoughts/**",
    "**/.git/**",
    "**/.github/**",
    "**/.husky/**",
    "**/scripts/**",
    "**/tests/**",
    "**/test-results/**",
    "**/playwright-report/**",
    "**/jscpd-report/**",
    "**/supabase/**",
    "**/CodeAnt/**",
    "**/errors/**",
    "**/*.md",
    "**/lib/database.types.ts"
  ],
  "gitignore": true
}
